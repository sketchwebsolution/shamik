/***************************************
****************************************
***********  Business ******************
*********** STYLE **********************/


.business-banner {
    padding: 10% 0;
    background-repeat: no-repeat;
    background-size: cover;
}
.banner-video {
	position: relative;
}
.vdo-btn {
	bottom: 0;
    color: #fff;
    font-size: 24px;
    font-weight: normal;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
}
.vdo_btn_innner {
	display: table;
    height: 100%;
    width: 100%;
}
.vdo_btn_innner a {
	display: table-cell;
    text-align: center;
    vertical-align: middle;
    
}
.bbaner-right {
	text-align: center;
}
.bbaner-right h1 {
	margin: 0 0 20px;
	letter-spacing: 2px;
}
.bbaner-right h4 {
	margin-bottom: 30px;
    margin-top: 20px;
}

.bcatgory {
	background: #353434;
	    padding: 40px 0;
}
.homerimg {
	display: none;
}
.busi_catlist {
	
}
.busi_catlist li {
	width: 20%;
	color: #fff;
	float: left;
}
.busi_catlist li img {
	-webkit-transform: scale(1);
    -ms-transform: scale(1);
    transform: scale(1);
    -webkit-transition: -webkit-transform 0.35s;
    transition: transform 0.35s;
}
.busi_catlist li:hover {
	color: #41c5eb;
}
.busi_catlist li:hover .homerimg{
	display: inline;
}
.busi_catlist li:hover .normalimg{
	display: none;
}
.busi_catlist li:hover img {
	-webkit-transform: scale(0.9);
    -ms-transform: scale(0.9);
    transform: scale(0.9);
}
.bcatgory .carousel-control {
	position: relative;
    display: inline-block;
    width: 8%;
    top: -4px;
    z-index: 99;
} 
.bcatgory .carousel-indicators {
	position: relative;
	bottom: -20px;
}
.bcatgory .carousel-indicators li {
	position: relative;
	z-index: 999;
}
.bcatgory .carousel-control.left,
.bcatgory .carousel-control.right {
	background-image: none;
}
.carcontroler {
	text-align: center;
}
.carcontroler span {
	position: relative;
}
.bcatgory .carousel-indicators .active {
	width: 10px;
    height: 10px;
    margin: 0;
    background-color: #47c5ed;
    border-color: #47c5ed;
}
.bcatgory .carousel-indicators li {
	background-color: #fff;
	width: 10px;
	height: 10px;
	margin: 0;
}
.bpol-right {
	background: #e8e8e8;
}
.bpol-title {
    margin: 0;
    padding: 40px 10px;
    font-size: 48px;
    line-height: 60px;
    font-weight: 700;
}
.bpolpara {
    padding: 0 40px 0 10px;
}
.catinvest, .pricetable, .standout, .bretarget {
	/*background-attachment: fixed;*/
	background-repeat: no-repeat;
	background-size: cover;
	padding: 40px 0;
	display: table;
    width: 100%;
}
.businesstitle {
    margin: 0;
}
.busiheader-title span {
    display: inline-block;
    width: 10%;
    height: 10px;
    background: #41c5eb;
    margin: 0 0 30px;
}
.busiheader-title {
    margin-bottom: 60px;
}
.catinvest-img {
    margin-bottom: 50px;
}
.catinvest-title {
    color: #fff;
    border-bottom: 2px solid #fff;
    padding-bottom: 30px;
    margin-bottom: 30px;
}
.cavlist {
	
}
.cavlist li {
	line-height: 28px;
}
.cavlist li a{
	color: #fff;
}
.cavlist li a:hover{
	color: #47c5ed;
}


/********** Table ***********/

.lptable {
	padding: 0 15px;
}
.tableheading {
	background: rgba(52,52,52,0.80);
    min-height: 100px;
    display: table;
    width: 100%;
}
.textdes {
	display: table-cell;
	vertical-align: middle;
}
.textdes .heading4 {
	font-size: 10px;
	padding: 0 13%;
}
.tablelist {
	
}
.tablelist li {
	background: rgba(255,255,255,0.80);
    color: #7b7b7b;
    height: 80px;
    line-height: 80px;
}
.tablelist li:nth-child(even) {
	background: rgba(234,234,234,0.80);
}
.buttonli.logbtn .full_btn {
	line-height: 20px;
}
.startup .tableheading{
	background: #343434;
} 
.enterprice .tableheading{
	background: rgba(71, 195, 232, 0.8);
}
.bpolcarousel .carousel-control.right,
.bpolcarousel .carousel-control.left {
	background-image: none;
}
.bpolcarousel .carousel-control {
	opacity: 1;
	width: 2%;
    font-size: 40px;
    color: #343434;
    text-shadow: none;
    top: 50%;
}
.bpolcarousel .carousel-control:hover {
	color: #333;
}
.bpolcarousel .carousel-control.right {
	right: -25px;
}
.bpolcarousel .carousel-control.left {
	left: -25px;
}
.videomadal .close {
    position: absolute;
    top: -10px;
    right: -10px;
    z-index: 999;
    width: 30px;
    height: 30px;
    background: #47c3e8;
    opacity: 1;
    border-radius: 50%;
    text-shadow: none;
    color: #fff;
}
.videomadal .modal-dialog {
	width: 85%;
	height: 92%;
	background: transparent url('../media/business-poster.jpg') 50% 50% / cover no-repeat ;
}
.videomadal .modal-dialog video[poster]{
	/*height:100%;*/
	width:100%;
	
}
