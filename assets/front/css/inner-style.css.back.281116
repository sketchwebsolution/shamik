/**************** Business Sell *************/

.bsell-search {
    background: #343434;
    padding: 60px 0;
    display: table;
    width: 100%;
}
.sellsearch .form-control {
	-webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    background: #fff url(../images/icon/select-arrow.png) 95% center no-repeat;
    height: 45px;
    position: relative;
}
.sellsearch .form-control:focus {
	box-shadow: none;
	border: 1px solid #41c5eb;
}
.sellsearch .form-group:after {
	position: absolute;
    content: '';
    width: 4px;
    height: 45px;
    background: #343434;
    right: 17%;
    bottom: 15px;
}
.bsell-search h2 {
	letter-spacing: 2px;
	margin-bottom: 40px;
}

