.crossbg:before {
    position: absolute;
    display: inline-block;
    content: '';
    top: 0px;
    bottom: 0px;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    transform: SkewY(-10deg);
    background: #fff;
    z-index: 0;
}
.aboutcontent {
	position: relative;
	z-index: 2;
}
