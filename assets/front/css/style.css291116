/********** Post ************/
.postimg {
	position: relative;
	overflow: hidden;
	background: #000;
}
.postimg img {
	/*max-width: none;
    height: 400px;*/
    width: 100%;
    height: 350px;
    -webkit-transition: opacity 1s, -webkit-transform 1s;
    transition: opacity 1s, transform 1s;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}
.postimg:hover img {
	opacity: 0.7;
    -webkit-transform: scale3d(1.1,1.1,1);
    transform: scale3d(1.1,1.1,1);
}
.post-title {
    bottom: 0;
    color: #fff;
    font-size: 24px;
    font-weight: normal;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
}
.pt-iner {
    display: table;
    height: 100%;
    width: 100%;
}
.pt-iner a {
	display: table-cell;
    text-align: center;
    vertical-align: middle;
    color: #fff;
    -webkit-transition: -webkit-transform 0.35s;
    transition: transform 0.35s;
    -webkit-transform: translate3d(0,0px,0);
    transform: translate3d(0,0px,0);
}
/*.postimg:hover .pt-iner a{
	-webkit-transform: translate3d(0,-20px,0);
    transform: translate3d(0,-20px,0);
}*/

.post-btn {
    position: absolute;
    bottom: 0;
    right: 0;
    z-index: 99;
    padding: 13px 28px;
    font-size: 14px;
   	-webkit-transition: -webkit-transform 0.35s;
    transition: transform 0.35s;
    -webkit-transform: translate3d(0,0,0);
    transform: translate3d(0,0,0);
}
.postimg:hover .post-btn{
	-webkit-transition: -webkit-transform 0.35s;
    transition: transform 0.35s;
    -webkit-transform: translate3d(0,-20px,0);
    transform: translate3d(0,-20px,0);
}
.post_dtls {
	/*margin-bottom: 30px;*/
	margin-bottom: 50px;
    padding: 0 10px;
}
.ppost li:last-child .post_dtls{
	margin-bottom: 0;
}


/************* footer ***********/

.footer {
	background: #343434 url('../images/footer-bg.jpg') center center no-repeat;
	padding: 40px 0 0;
	color: #fff;
}
.footer-title {
    font-size: 24px;
    font-weight: 600;
    margin: 0px;
    letter-spacing: 2px;
}
.footer-widget li{
	font-size: 14px;
    line-height: 24px;
    font-weight: 400;
    text-transform: uppercase;
}
.footer-top {
    margin-bottom: 45px;
}
.footer-widget li:hover a{
	color: #41c5eb;
}
.footer-widget li a {
	color: #f7f7f7;
}
.footer-widget li a span {
	position: relative;
	z-index: 2;
	text-decoration: none;
	-webkit-touch-callout: none;
	-webkit-tap-highlight-color: transparent;
	padding: 0 0 7px 0;
	margin-bottom: 8px;
	white-space: nowrap;
	overflow: hidden;
	display: table;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-transition: color 1s ease;
	-moz-transition: color 1s ease;
	-ms-transition: color 1s ease;
	-o-transition: color 1s ease;
	transition: color 1s ease;
}
.footer-widget li a span:hover {
	color: #41c5eb;
    -webkit-transition: color 1s ease;
    -moz-transition: color 1s ease;
    -ms-transition: color 1s ease;
    -o-transition: color 1s ease;
    transition: color 1s ease;
    cursor: pointer;
}
.footer-widget li a span:after {
    content: "";
    width: 2px;
    height: 2px;
    left: 0;
    bottom: 0;
    position: absolute;
}
.footer-widget li a span:focus:after, 
.footer-widget li a span:hover:after {
    width: 100%;
    height: 2px;
    background-color: #1B98E0;
    -webkit-transition: all 1s ease 0s;
    -moz-transition: all 1s ease 0s;
    -ms-transition: all 1s ease 0s;
    -o-transition: all 1s ease 0s;
    transition: all 1s ease 0s;
}
