/************ Sidebar Style ************/

.nav-side-menu {
  overflow: auto;
  font-weight: 200;
  background-color: #f7f8f8;
  top: 0px;
  width: 100%;
  height: 100%;
  color: #e1ffff;
  margin-top:24px;
}
.nav-side-menu .brand {
  background-color: #23282e;
  line-height: 38px;
  display: none;
  text-align: center;
  font-size: 14px;
}
.nav-side-menu .toggle-btn {
  display: none;
}
.nav-side-menu ul {
	margin-bottom: 30px;
}
.nav-side-menu ul,
.nav-side-menu li {
  list-style: none;
  line-height: 35px;
  cursor: pointer;
}
.nav-side-menu ul a > strong,
.nav-side-menu li > a > strong{
    font-size: 1.0em;
    font-weight: 400;
    vertical-align: middle;
}
.nav-side-menu ul :not(collapsed) .arrow:before,
.nav-side-menu li :not(collapsed) .arrow:before {
  font-family: FontAwesome;
  /*content: "\f078";*/
  content:"\f107";
  display: inline-block;
  padding-left: 10px;
  padding-right: 10px;
  vertical-align: middle;
  float: right;
