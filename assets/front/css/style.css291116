.search-container {
	display: block;
	margin: 60px 0 0;
	position: relative;
}
#video-background {
  position: fixed;
  right: 0;
  bottom: 0;
  min-width: 100%;
  min-height: 100%;
  width: auto;
  height: auto;
  z-index: -100;
}

input#search-bar {
	margin: 0 auto;
	width: 100%;
	height: 45px;
	padding: 0 20px;
	font-size: 14px;
	border: 1px solid transparent;
	outline: none;
	border-radius: 3px;
}
input#search-bar:focus {
	border: 1px solid #47c3e9;
	transition: 0.35s ease;
	color: #008ABF;
}
input#search-bar:focus::-webkit-input-placeholder {
  transition: opacity 0.45s ease;
  opacity: 0;
}
input#search-bar:focus::-moz-placeholder {
  transition: opacity 0.45s ease;
  opacity: 0;
}
input#search-bar:focus:-ms-placeholder {
  transition: opacity 0.45s ease;
  opacity: 0;
}
.banner-section {
	background-size: cover;
    background-position: 0% 100%;
    background-repeat: no-repeat;
}
.search-icon {
	position: absolute;
	top: 10px;
	right: 13px;
	z-index: 99;
}
.searchform {
	margin-top: 55px;
}
.searchform .form-group .form-control{
	border-radius: 4px 0 0 4px;
	border: none;
	width: 100%;
	height: 45px;
	padding: 0 20px;
	font-size: 1rem;
	border: 1px solid #D0CFCE;
	outline: none;
}
.searchform .input-group-addon {
	background-color: #fff;
	font-size: 16px;
	border: none;
}
.input-group {
	
}


/*********** Categories **************/

.catgory-search {
    background: #353533;
    padding: 15px 0;
}
.catgory-search .nav-tabs > li {
	border-right: 1px solid #565656;
}
.catgory-search .nav-tabs > li:last-child {
	border-right: none;
}
.catgory-search .nav-tabs > li > a {
	border: none;
	border-radius: 0;
}
.catgory-search .nav-tabs > li > a > img {
	margin-right: 30px;
	-webkit-transform: scale(1);
	-ms-transform: scale(1);
	transform: scale(1);
	-webkit-transition: -webkit-transform 0.35s;
    transition: transform 0.35s;
    width: 42px;
}
.catgory-search .nav > li > a:hover, 
.catgory-search .nav > li > a:focus {
	background-color: transparent;
}
.catgory-search .nav > li > a:hover img  {
	-webkit-transform: scale(0.9);
	-ms-transform: scale(0.9);
	transform: scale(0.9);
}
.catgory-search .nav > li > a {
	padding: 0px 15px;
}


/*********** Exprence Section **********/

.exprence_section {
    background: #f5f6f8;
    padding: 0px;
    display: table;
    width: 100%;
    margin: 50px 0 0;
    color: #343432;
}
.page-secondtitle {
	margin: 0;
    font-size: 24px;
    font-weight: 600;
    line-height: 30px;
    /*letter-spacing: 2px;*/
} 
.exp_left {
	padding: 40px;
}
.exp_left p{
	font-size: 14px;
    color: #8b8b8b;
    line-height: 24px;
    margin: 25px 0 10%;
    padding-right: 25%;
}
.full_btn {
	padding: 15px;
	font-size: 14px;
	webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-tap-highlight-color: transparent;
    vertical-align: middle;
    z-index: 1;
    will-change: opacity, transform;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
    -o-transition: all 0.3s ease-out;
    -ms-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
}
.full_btn:hover {
	box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18), 0 4px 15px 0 rgba(0, 0, 0, 0.15);
}
.all_btn {
	font-size: 14px;
	padding: 6px 12px;
	webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    -webkit-tap-highlight-color: transparent;
    vertical-align: middle;
    z-index: 1;
    will-change: opacity, transform;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
    -o-transition: all 0.3s ease-out;
    -ms-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
}
.all_btn:hover {
	box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18), 0 4px 15px 0 rgba(0, 0, 0, 0.15);
}


/*********** Popular Post ***********/
.title-header {
	margin: 50px 0;
}
.title-header p {
